# =====================================================
# ALB MASTER - AMBIENTE DESARROLLO
# =====================================================

# Configuración básica
ALB_MASTER_ENABLED=true
ENVIRONMENT=dev1
NAMESPACE=dev1

# ALB Configuration
ALB_GROUP_NAME=xxxxxxx
ALB_LOAD_BALANCER_NAME=dev-master-alb
ALB_SCHEME=internal
ALB_LISTEN_PORTS='[{"HTTP":80},{"HTTPS":443},{"HTTPS":8443}]'
ALB_HEALTHCHECK_PATH=/
ALB_SUBNETS=subnet-xxxxx,subnet-xxxxx 
ALB_SECURITY_GROUPS=sg-xxxxxx

# SSL/TLS (deshabilitado en dev)
SSL_ENABLED=true
SSL_REDIRECT_ENABLED=false
SSL_REDIRECT_PORT=443
SSL_CERTIFICATE_ARN=arn:aws:acm:us-east-1:xxxxxxxx:certificate/xxxxxx-xxxxxx-xxxx-xxxxx-xxxxxxx
SSL_POLICY=ELBSecurityPolicy-TLS13-1-2-2021-06

# mTLS (deshabilitado en dev)
MTLS_ENABLED=true
MTLS_MUTUAL_AUTHENTICATION='[{"port": xxxx, "mode": "verify", "trustStore": "arn:aws:elasticloadbalancing:us-east-1:xxxxxx:truststore/trust-store-xxxxx/xxxxxxx", "ignoreClientCertificateExpiry": false}]'

# WAF (deshabilitado en dev)
WAF_ENABLED=false
WAF_ACL_ARN=arn:aws:wafv2:us-west-2:123456789012:regional/webacl/prod-waf/prod-acl-id

# Default Backend
DEFAULT_BACKEND_ENABLED=true
DEFAULT_BACKEND_REPLICAS=1