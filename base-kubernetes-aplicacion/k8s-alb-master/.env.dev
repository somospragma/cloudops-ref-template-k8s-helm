# =====================================================
# ALB MASTER - AMBIENTE DESARROLLO
# =====================================================

# Configuración básica
ALB_MASTER_ENABLED=true
ENVIRONMENT=dev1
NAMESPACE=dev1

# ALB Configuration
ALB_GROUP_NAME=dev-alb-group
ALB_LOAD_BALANCER_NAME=dev-master-alb
ALB_SCHEME=internal
ALB_LISTEN_PORTS='[{"HTTP":80},{"HTTPS":443},{"HTTPS":8443}]'
ALB_HEALTHCHECK_PATH=/health
ALB_SUBNETS=subnet-12345678,subnet-23456789
ALB_SECURITY_GROUPS=sg-12345678,sg-23456789

# SSL/TLS (deshabilitado en dev)
SSL_ENABLED=true
SSL_REDIRECT=8443
SSL_CERTIFICATE_ARN=arn:aws:acm:us-west-2:123456789012:certificate/12345678-1234-1234-1234-123456789012
SSL_POLICY=ELBSecurityPolicy-TLS13-1-2-2021-06

# mTLS (deshabilitado en dev)
MTLS_ENABLED=true
MTLS_MUTUAL_AUTHENTICATION="[{"port": 443, "mode": "verify", "trustStore": "arn:aws:elasticloadbalancing:us-east-1:161156235207:truststore/trust-store-poc2/299a875c96c72541", "ignoreClientCertificateExpiry": false}]"

# WAF (deshabilitado en dev)
WAF_ENABLED=true
WAF_ACL_ARN=arn:aws:wafv2:us-west-2:123456789012:regional/webacl/prod-waf/prod-acl-id

# Default Backend
DEFAULT_BACKEND_ENABLED=true
DEFAULT_BACKEND_REPLICAS=1